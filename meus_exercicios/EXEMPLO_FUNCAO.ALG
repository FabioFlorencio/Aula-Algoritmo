algoritmo "FUNCAO2"
var
   V1,V2,S:INTEIRO
   
FUNCAO SOMA(X,Y:INTEIRO):INTEIRO
INICIO
   RETORNE X+Y
FIMFUNCAO

inicio
   ESCREVA("DIGITE O 1° PRIMEIRO:")
   LEIA(V1)
   ESCREVA("DIGITE O 2° NUMERO:")
   LEIA(V2)
   S<-SOMA(V1,V2)
   ESCREVAL("A SOMA DOS VALORES ",V1," E ",V2," E IGUAL A ",S)

fimalgoritmo