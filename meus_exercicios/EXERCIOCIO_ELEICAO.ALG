Algoritmo "semnome"

Var
 cad1,cad2,cad3,votoTotal:inteiro


Inicio
  cad1 <- 0
  cad2 <- 0
  cad3 <- 0
  escreval("Quantas pessoas vão participar da votação ?")
  leia(votoTotal)
  Escreval( "Total de eleitores são:",(votoTotal))
  Escreval("Escolha seu candidato entre as opções abaixo:")
  escreval("CAD1,CAD2,CAD3")
  leia(cad1,cad2,cad3)
      (CAD1 <- CAD1 + 0)


Fimalgoritmo