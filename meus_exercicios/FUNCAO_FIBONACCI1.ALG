algoritmo "FUNCAO_FIBONACCI1"
var
   I,T1,T2,T3:INTEIRO
FUNCAO FIBONACCI(VAR N1,N2:INTEIRO):INTEIRO
VAR
   N3:INTEIRO
INICIO
   N3<-N1+N2
   N1<-N2
   N2<-N3
   RETORNE N3
FIMFUNCAO

inicio
   T1<-0
   ESCREVA(T1)
   T2<-1
   ESCREVA(T2)
   PARA I DE 1 ATE 8 FACA
      T3<-FIBONACCI(T1,T2)
      ESCREVA(T3)
   FIMPARA
fimalgoritmo